{
  "name": "@mozaik/ext-travis",
  "version": "2.0.0-rc.1",
  "description": "Mozaik travis widgets",
  "repository": {
    "type": "git",
    "url": "git://github.com/plouc/mozaik-ext-travis"
  },
  "license": "MIT",
  "author": {
    "name": "RaphaÃ«l Benitte",
    "url": "https://github.com/plouc"
  },
  "homepage": "https://github.com/plouc/mozaik-ext-travis",
  "main": "./lib/components/index.js",
  "module": "es/components/index.js",
  "jsnext:main": "es/components/index.js",
  "keywords": [
    "travis",
    "ci",
    "mozaik",
    "widget",
    "extension",
    "dashboard"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "@nivo/bar": "^0.42.1",
    "@nivo/pie": "^0.42.1",
    "chalk": "^2.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@mozaik/babel-preset": "^1.0.0-alpha.6",
    "@mozaik/ui": "^2.0.0-rc.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "nyc": "^11.1.0",
    "prettier": "^1.14.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "peerDependencies": {
    "@mozaik/ui": "^2.0.0-rc.2",
    "react": "^16.4.0"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.js ./test/**/*.js",
    "lint:fix": "eslint --fix ./src/**/*.js ./test/**/*.js",
    "test": "jest --verbose",
    "test:cover": "jest --verbose --coverage",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "cover-report": "nyc report --reporter=lcov && open coverage/lcov-report/index.html",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:commonjs:watch": "npm run build:commonjs -- --watch",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:es:watch": "npm run build:es -- --watch",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublishOnly": "npm run build",
    "fmt": "prettier --color --write \"{src,test,client}/**/*.{js,json}\"",
    "fmt:check": "prettier --list-different \"{src,test,client}/**/*.{js,json}\"",
    "version": "echo ${npm_package_version}",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>test/setupTests.js",
    "testURL": "http://localhost/"
  }
}
